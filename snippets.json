{
	"Function": {
		"prefix": ["function"],
		"body": [
			"func ${1:name}($2){",
			"    $0",
			"}"
		],
		"description": "function snippet"
	},

	"if statement": {
		"prefix": "if",
		"body": [
			"if ${1:condition}{",
			"    $0",
			"}"
		],
		"description": "if statement"
	},
	"if elseif else statement ": {
		"prefix": "if elseif else",
		"body": [
			"if ${1:condition}{",
			"    $2",
            "}",
			"elseif ${3:condition}{",
			"    $4",
            "}",
			"else {",
			"    $0",
			"}\n"
		],
		"description": "if, elseif, else statement"
	},
	"if else statement": {
		"prefix": "if else",
		"body": [
			"if ${1:condition}{",
			"    $2",
            "}",
			"else {",
			"    $0",
			"}\n"
		],
		"description": "if, else statement"
	},
	"for loop":{
		"prefix":["for"],
		"body": [
			"for ${1:condition}{",
			"    $0",
			"}"
		]
	},
	"while loop":{
		"prefix":["while"],
		"body": [
			"while ${1:condition}{",
			"    $0",
			"}"
		]
	},
	"import statement":{
		"prefix":["import","#import"],
		"body": [
			"#import ${0:library}"
		]
	},
    "import statement with alias":{
        "prefix":["import","#import"],
        "body": [
            "#import ${1:library} as ${0:alias}"
        ]
    },
    "class":{
        "prefix":["class"],
        "body": [
            "class ${1:name}($2){",
            "    $0",
            "}"
        ]
    },
    "class with superclass":{
        "prefix":["class"],
        "body": [
            "class ${1:name}($2) inherits ${3:superclass}{",
            "    $0",
            "}\n"
        ]
    }
}