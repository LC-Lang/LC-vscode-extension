{
    "Function": {
        "prefix": [
            "function"
        ],
        "body": [
            "func ${1:name}($2){",
            "    $0",
            "}"
        ],
        "description": "function snippet"
    },
    "if statement": {
        "prefix": "if",
        "body": [
            "if ${1:condition}{",
            "    $0",
            "}"
        ],
        "description": "if statement"
    },
    "if elseif else statement ": {
        "prefix": "if else if else",
        "body": [
            "if ${1:condition}{",
            "    $2",
            "}",
            "else if ${3:condition}{",
            "    $4",
            "}",
            "else {",
            "    $0",
            "}\n"
        ],
        "description": "if, elseif, else statement"
    },
    "if else statement": {
        "prefix": "if else",
        "body": [
            "if ${1:condition}{",
            "    $2",
            "}",
            "else {",
            "    $0",
            "}\n"
        ],
        "description": "if, else statement"
    },
    "for loop": {
        "prefix": [
            "for"
        ],
        "body": [
            "for ${1:condition}{",
            "    $0",
            "}"
        ]
    },
    "while loop": {
        "prefix": [
            "while"
        ],
        "body": [
            "while ${1:condition}{",
            "    $0",
            "}"
        ]
    },
    "import statement": {
        "prefix": [
            "import"
        ],
        "body": [
            "import ${0:package}"
        ]
    },
    "import statement with alias": {
        "prefix": [
            "import"
        ],
        "body": [
            "import ${1:package} as ${0:alias}"
        ]
    },
    "class": {
        "prefix": [
            "class"
        ],
        "body": [
            "class ${1:name}($2){",
            "    $0",
            "}"
        ]
    },
    "class with superclass": {
        "prefix": [
            "class"
        ],
        "body": [
            "class ${1:name}($2) inherits ${3:parentclass}{",
            "    $0",
            "}\n"
        ]
    }
}